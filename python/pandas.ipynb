    "import pandas as pd\n",
    "\n",
    "df = pd.DataFrame({'X': ['A', 'B', 'A', 'B'], 'Y': [1, 4, 3, 2]})\n",
    "print(\"\\nOriginal DF:\")\n",
    "print(df)\n",
    "\n",
    "grouped = df.groupby('X')\n",
    "\n",
    "print(\"\\nSum:\")\n",
    "print(grouped.sum())\n",
    "\n",
    "# iteration\n",
    "for name, group in grouped:\n",
    "    print(\"\\nGroup\", name + \":\")\n",
    "    print(group)"
